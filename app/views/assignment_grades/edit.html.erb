<h1>Edit Grade</h1>

<%=form_for @grade do |f|%>
  <p>
    <%=f.label :Assignment%><br/>
    <%=f.text_field :assignment_id%>
  </p>

  <p>
    <%=f.label :Student%><br/>
    <%=f.text_field :user_id%>
  </p>

  <p>
    <%=f.label :Grade%><br/>
    <%=f.text_field :grade_value%>
  </p>

  <p>
    <%=f.submit "Edit Grade"%>
  </p>

<%end%>

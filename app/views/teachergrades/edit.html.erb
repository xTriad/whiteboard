<h1>Edit Grade</h1>

<%=form_for @grade do |f|%>
  <p>
    <%=f.label :Assignment%><br/>
    <%=f.text_field :aname%>
  </p>

  <p>
    <%=f.label :Student%><br/>
    <%=f.text_field :sname%>
  </p>

  <p>
    <%=f.label :points%><br/>
    <%=f.text_field :points%>
  </p>

  <p>
    <%=f.label :PoinsPossible%><br/>
    <%=f.text_field :pointsposs%>
  </p>

  <p>
    <%=f.label :comment%><br/>
    <%=f.text_area :comment%>
  </p>

  <p>
    <%=f.submit "Edit Grade"%>
  </p>

<%end%>
